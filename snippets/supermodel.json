{
    "Set Widget": {
        "prefix": "plonewidget",
        "body":[
            "<form:widget type=\"${1|z3c.form.browser.text.TextWidget,z3c.form.browser.checkbox.CheckBoxFieldWidget,z3c.form.browser.checkbox.SingleCheckBoxFieldWidget,plone.z3cform.textlines.TextLinesFieldWidget,plone.app.z3cform.wysiwyg.WysiwygFieldWidget,plone.app.z3cform.widget.SelectFieldWidget,plone.app.z3cform.widget.AjaxSelectFieldWidget,plone.app.z3cform.widget.RelatedItemsFieldWidget,plone.app.z3cform.widget.QueryStringFieldWidget,plone.app.z3cform.widget.LinkFieldWidget|}\">",
            "  <klass>${2:my_css_class}</klass>",
            "</form:widget>"
        ]
    },
    "RelationChoice (single)": {
        "prefix": "plonerelationchoice",
        "body":[
            "<field name=\"${1}\" type=\"${2:z3c.relationfield.schema.RelationChoice}\">",
            "  <title>${3}</title>",
            "  <portal_type>",
            "    <element>${4|Document,News Item,Event,File,Image,Link,Collection,Folder|}</element>",
            "    <element>${5|Document,News Item,Event,File,Image,Link,Collection,Folder|}</element>",
            "  </portal_type>",
            "  <required>${6|False,True|}</required>",
            "  <readonly>False</readonly>",
            "</field>"
        ]
    },
    "RelationList (multi)": {
        "prefix": "plonerelationlist",
        "body":[
            "<field name=\"${1:name}\" type=\"${2:z3c.relationfield.schema.RelationList}\">",
            "  <title>${3:Related Items}</title>",
            "  <value_type type=\"z3c.relationfield.schema.RelationChoice\">",
            "    <title>${5:Related}</title>",
            "    <portal_type>",
            "      <element>${6|Document,News Item,Event,File,Image,Link,Collection,Folder|}</element>",
            "      <element>${7|Document,News Item,Event,File,Image,Link,Collection,Folder|}</element>",
            "    </portal_type>",
            "  </value_type>",
            "  <required>${8|False,True|}</required>",
            "  <readonly>False</readonly>",
            "</field>"
        ]
    },
